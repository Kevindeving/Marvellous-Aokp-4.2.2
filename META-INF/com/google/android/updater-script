ui_print("Mounting system...");
mount("yaffs2", "MTD", "system", "/system");
set_progress(0.15);

###REQUIRED SYSTEM FILES
##
ui_print("Installing System Apps");	
package_extract_dir("system", "/system");	
set_progress(0.5);	
#-
#- Installing GPS tweaks
#-
ui_print("@GPS Configuration...");

#Angola
if file_getprop("/tmp/aroma-data/gps.prop","selected.1") == "1" then
ui_print("Setting up GPS for your country...");
delete("/system/etc/gps.conf");
ui_print("Copying gps.conf...");
package_extract_dir("stuff/gpsconf/Angola/system", "/system");
ui_print("gps.conf file updated, the gps is now more accurate and locks faster...");
endif;

